im2.new(0,0,0,60);whitelistPage.BackgroundColor3=Color3.fromRGB(25,25,25);whitelistPage.CanvasSize=UDim2.new(0,0,0,0);whitelistPage.ScrollBarThickness=8;whitelistPage.Visible=false;whitelistPage.Parent=mainFrame;featureTabBtn.MouseButton1Click:Connect(function() featurePage.Visible=true;whitelistPage.Visible=false;end);whitelistTabBtn.MouseButton1Click:Connect(function() featurePage.Visible=false;whitelistPage.Visible=true;end);local function createFeature(name,posY,defaultActive) local btn=Instance.new("TextButton");btn.Size=UDim2.new(0,120,0,40);btn.Position=UDim2.new(0,10,0,posY);btn.Text=name   .. ": "   .. ((defaultActive and "ON") or "OFF") ;btn.BackgroundColor3=Color3.fromRGB(15,15,15);btn.TextColor3=Color3.fromRGB(255,255,255);btn.Parent=featurePage;local keyBox=Instance.new("TextBox");keyBox.Size=UDim2.new(0,100,0,40);keyBox.Position=UDim2.new(0,140,0,posY);keyBox.PlaceholderText=name   .. " Keybind" ;keyBox.ClearTextOnFocus=false;keyBox.Text="";keyBox.BackgroundColor3=Color3.fromRGB(15,15,15);keyBox.TextColor3=Color3.fromRGB(255,255,255);keyBox.Parent=featurePage;local active=defaultActive or false ;local key=nil;keyBox.FocusLost:Connect(function() local keyName=keyBox.Text:upper();local success,newKey=pcall(function() return Enum.KeyCode[keyName];end);if (success and newKey) then key=newKey;end end);local function toggleActive() active= not active;btn.Text=name   .. ((active and ": ON") or ": OFF") ;end btn.MouseButton1Click:Connect(toggleActive);UserInputService.InputBegan:Connect(function(input,processed) if ( not processed and key and (input.KeyCode==key)) then toggleActive();end end);return function() return active;end;end local isTeleportActive=createFeature("Teleport",40,false);local isRapidFireActive=createFeature("Rapid Fire",90,true);local isFlyActive=createFeature("Fly",140,false);local isCamlockActive=createFeature("Camlock",190,false);local isESPActive=createFeature("ESP",240,true);local isInfAmmoActive=createFeature("Inf Ammo",290,true);local flySpeedBox=Instance.new("TextBox");flySpeedBox.Size=UDim2.new(0,100,0,40);flySpeedBox.Position=UDim2.new(0,260,0,140);flySpeedBox.PlaceholderText="Fly Speed";flySpeedBox.ClearTextOnFocus=false;flySpeedBox.Text="";flySpeedBox.BackgroundColor3=Color3.fromRGB(15,15,15);flySpeedBox.TextColor3=Color3.fromRGB(255,255,255);flySpeedBox.Parent=featurePage;local function createPlayerFrame(player,y) if whitelistPage:FindFirstChild(player.Name) then return whitelistPage[player.Name];end local plrFrame=Instance.new("Frame");plrFrame.Name=player.Name;plrFrame.Size=UDim2.new(1, -20,0,60);plrFrame.Position=UDim2.new(0,10,0,y);plrFrame.BackgroundColor3=Color3.fromRGB(35,35,35);plrFrame.Parent=whitelistPage;local thumb=Players:GetUserThumbnailAsync(player.UserId,Enum.ThumbnailType.HeadShot,Enum.ThumbnailSize.Size60x60);local img=Instance.new("ImageLabel");img.Size=UDim2.new(0,60,0,60);img.Image=thumb;img.Parent=plrFrame;local nameLbl=Instance.new("TextLabel");nameLbl.Size=UDim2.new(0.5,0,1,0);nameLbl.Position=UDim2.new(0,70,0,0);nameLbl.Text=player.Name;nameLbl.TextColor3=Color3.fromRGB(255,255,255);nameLbl.BackgroundTransparency=1;nameLbl.Parent=plrFrame;local btn=Instance.new("TextButton");btn.Size=UDim2.new(0.3,0,0.8,0);btn.Position=UDim2.new(0.7,0,0.1,0);btn.Parent=plrFrame;if (SAFE_PLAYERS[player.Name]==nil) then SAFE_PLAYERS[player.Name]=true;end local function updateBtn() if SAFE_PLAYERS[player.Name] then btn.Text="Whitelisted";btn.TextColor3=Color3.fromRGB(0,255,0);btn.BackgroundColor3=Color3.fromRGB(0,70,0);else btn.Text="Whitelist";btn.TextColor3=Color3.fromRGB(255,0,0);btn.BackgroundColor3=Color3.fromRGB(70,0,0);end end updateBtn();btn.MouseButton1Click:Connect(function() SAFE_PLAYERS[player.Name]= not SAFE_PLAYERS[player.Name];updateBtn();end);return plrFrame;end local function refreshWhitelist() local y=10;for _,player in ipairs(Players:GetPlayers()) do local frame=createPlayerFrame(player,y);if frame then frame.Position=UDim2.new(0,10,0,y);y=y + 70 ;end end whitelistPage.CanvasSize=UDim2.new(0,0,0,y);end refreshWhitelist();Players.PlayerAdded:Connect(function() refreshWhitelist();end);Players.PlayerRemoving:Connect(function(player) if whitelistPage:FindFirstChild(player.Name) then whitelistPage[player.Name]:Destroy();end SAFE_PLAYERS[player.Name]=nil;refreshWhitelist();end);RunService.RenderStepped:Connect(function() if isTeleportActive() then local char=LocalPlayer.Character;if (char and char:FindFirstChild("HumanoidRootPart")) then local hrp=char.HumanoidRootPart;for _,player in ipairs(Players:GetPlayers()) do if ((player~=LocalPlayer) and  not SAFE_PLAYERS[player.Name]) then local pChar=player.Character;if (pChar and pChar:FindFirstChild("HumanoidRootPart")) then pChar.HumanoidRootPart.CFrame=hrp.CFrame * CFrame.new(0,0, -8) ;end end end end end end);UserInputService.InputBegan:Connect(function(input,gp) if gp then return;end if (input.UserInputType==Enum.UserInputType.MouseButton1) then mouseDown=true;end end);UserInputService.InputEnded:Connect(function(input) if (input.UserInputType==Enum.UserInputType.MouseButton1) then mouseDown=false;end end);RunService.Heartbeat:Connect(function() local char=LocalPlayer.Character;if  not char then return;end local tool=char:FindFirstChildOfClass("Tool");if (tool and tool:FindFirstChild("GunScript")) then if isInfAmmoActive() then local ammo=tool:FindFirstChild("Ammo");if (ammo and ammo:IsA("IntValue")) then ammo.Value=9999;end end if (isRapidFireActive() and mouseDown) then pcall(function() tool:Activate();end);end end end);local function startFly() local hrp=LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") ;if  not hrp then return;end velocity=Instance.new("BodyVelocity",hrp);velocity.MaxForce=Vector3.new(1000000000,1000000000,1000000000);velocity.Velocity=Vector3.new();gyro=Instance.new("BodyGyro",hrp);gyro.MaxTorque=Vector3.new(1000000000,1000000000,1000000000);gyro.P=10000;flying=true;end local function stopFly() flying=false;if velocity then velocity:Destroy();velocity=nil;end if gyro then gyro:Destroy();gyro=nil;end end flySpeedBox.FocusLost:Connect(function() local val=tonumber(flySpeedBox.Text);if (val and (val>0)) then flySpeed=val;end end);RunService.RenderStepped:Connect(function() if (isFlyActive() and (flySpeed>0)) then local char=LocalPlayer.Character;if (char and char:FindFirstChild("HumanoidRootPart")) then local hrp=char.HumanoidRootPart;if  not flying then startFly();end local cf=Camera.CFrame;local moveVec=Vector3.new();if UserInputService:IsKeyDown(Enum.KeyCode.W) then moveVec+=cf.LookVector end if UserInputService:IsKeyDown(Enum.KeyCode.S) then moveVec-=cf.LookVector end if UserInputService:IsKeyDown(Enum.KeyCode.A) then moveVec-=cf.RightVector end if UserInputService:IsKeyDown(Enum.KeyCode.D) then moveVec+=cf.RightVector end if UserInputService:IsKeyDown(Enum.KeyCode.Space) then moveVec+=cf.UpVector end if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then moveVec-=cf.UpVector end velocity.Velocity=((moveVec.Magnitude>0) and (moveVec.Unit * flySpeed)) or Vector3.new() ;gyro.CFrame=cf;end elseif flying then stopFly();end end);local function addESP(player) if (player==LocalPlayer) then return;end local function applyESP(char) if  not playerTags[player] then local tag=Instance.new("BillboardGui");tag.Size=UDim2.new(0,100,0,50);tag.Adornee=char:WaitForChild("HumanoidRootPart");tag.AlwaysOnTop=true;local txt=Instance.new("TextLabel",tag);txt.Size=UDim2.new(1,0,1,0);txt.BackgroundTransparency=1;txt.TextColor3=Color3.fromRGB(255,0,0);txt.TextStrokeTransparency=0;txt.Text=player.Name;tag.Parent=char;playerTags[player]=tag;end end if player.Character then applyESP(player.Character);end player.CharacterAdded:Connect(applyESP);end RunService.RenderStepped:Connect(function() if isESPActive() then for _,player in ipairs(Players:GetPlayers()) do if  not playerTags[player] then addESP(player);end end else for p,tag in pairs(playerTags) do if tag then tag:Destroy();end playerTags[p]=nil;end end end);Players.PlayerRemoving:Connect(function(player) if playerTags[player] then playerTags[player]:Destroy();playerTags[player]=nil;end end);RunService.RenderStepped:Connect(function() if isCamlockActive() then local mousePos=UserInputService:GetMouseLocation();if ( not lockedPlayer or  not lockedPlayer.Character) then local target=nil;local smallestDist=math.huge;for _,player in ipairs(Players:GetPlayers()) do if ((player~=LocalPlayer) and player.Character and player.Character:FindFirstChild("HumanoidRootPart")) then local screenPos,onScreen=Camera:WorldToViewportPoint(player.Character.HumanoidRootPart.Position);if onScreen then local dist=(Vector2.new(screenPos.X,screenPos.Y) -Vector2.new(mousePos.X,mousePos.Y)).Magnitude;if (dist<smallestDist) then smallestDist=dist;target=player;end end end end lockedPlayer=target;if (lockedPlayer and  not aura) then aura=Instance.new("BoxHandleAdornment");aura.Adornee=lockedPlayer.Character:FindFirstChild("HumanoidRootPart");aura.Color3=Color3.fromRGB(0,170,255);aura.AlwaysOnTop=true;aura.Size=Vector3.new(2,3,1);aura.Parent=lockedPlayer.Character;end end if (lockedPlayer and lockedPlayer.Character and lockedPlayer.Character:FindFirstChild("HumanoidRootPart")) then Camera.CFrame=CFrame.new(Camera.CFrame.Position,lockedPlayer.Character.HumanoidRootPart.Position);if aura then aura.Adornee=lockedPlayer.Character.HumanoidRootPart;end end else if aura then aura:Destroy();aura=nil;end lockedPlayer=nil;end end); end
